    <!-- Document Title -->
    <title>IT Assets Storage Management</title>
    <meta name="description" content="Professional IT Asset Storage Management Dashboard">
    <meta name="author" content="IT Assets Team">

    <!-- Favicon -->
    <link rel="icon" type="image/png" sizes="512x512" href="~/layoutAssets/assets/favicon/favicon-512x512.png">

