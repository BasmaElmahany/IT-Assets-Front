    <!-- CSS Files -->
    <link rel="stylesheet" href="~/layoutAssets/assets/css/console-bsb.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/jsvectormap/dist/css/jsvectormap.min.css">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />

    <!-- Professional Storage Dashboard Styles -->
    <link rel="stylesheet" href="~/layoutAssets/assets/css/storage-dashboard-professional.css">
    <style>
        /* ========================================
           PROFESSIONAL STORAGE DASHBOARD
           Modern, Sophisticated Design System
           ======================================== */

        /* ========================================
           COLOR SYSTEM - Professional Palette
           ======================================== */

        :root {
            /* Primary - Deep Navy Blue */
            --primary: #0F172A;
            --primary-light: #1E293B;
            --primary-lighter: #334155;
            /* Accent - Vibrant Cyan */
            --accent: #06B6D4;
            --accent-light: #22D3EE;
            --accent-dark: #0891B2;
            /* Secondary - Purple */
            --secondary: #8B5CF6;
            --secondary-light: #A78BFA;
            --secondary-dark: #7C3AED;
            /* Success - Emerald */
            --success: #10B981;
            --success-light: #34D399;
            --success-dark: #059669;
            /* Warning - Amber */
            --warning: #F59E0B;
            --warning-light: #FBBF24;
            --warning-dark: #D97706;
            /* Danger - Rose */
            --danger: #EF4444;
            --danger-light: #F87171;
            --danger-dark: #DC2626;
            /* Neutrals - Slate */
            --gray-50: #F8FAFC;
            --gray-100: #F1F5F9;
            --gray-200: #E2E8F0;
            --gray-300: #CBD5E1;
            --gray-400: #94A3B8;
            --gray-500: #64748B;
            --gray-600: #475569;
            --gray-700: #334155;
            --gray-800: #1E293B;
            --gray-900: #0F172A;
            /* Spacing */
            --space-xs: 0.25rem;
            --space-sm: 0.5rem;
            --space-md: 1rem;
            --space-lg: 1.5rem;
            --space-xl: 2rem;
            --space-2xl: 3rem;
            /* Border Radius */
            --radius-sm: 0.375rem;
            --radius-md: 0.5rem;
            --radius-lg: 0.75rem;
            --radius-xl: 1rem;
            --radius-2xl: 1.5rem;
            /* Shadows */
            --shadow-sm: 0 1px 2px 0 rgba(0, 0, 0, 0.05);
            --shadow-md: 0 4px 6px -1px rgba(0, 0, 0, 0.1);
            --shadow-lg: 0 10px 15px -3px rgba(0, 0, 0, 0.1);
            --shadow-xl: 0 20px 25px -5px rgba(0, 0, 0, 0.1);
            --shadow-2xl: 0 25px 50px -12px rgba(0, 0, 0, 0.25);
            /* Transitions */
            --transition-fast: 150ms cubic-bezier(0.4, 0, 0.2, 1);
            --transition-base: 300ms cubic-bezier(0.4, 0, 0.2, 1);
            --transition-slow: 500ms cubic-bezier(0.4, 0, 0.2, 1);
        }

        /* ========================================
           GLOBAL STYLES
           ======================================== */

        body {
            font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
            background: var(--gray-50);
            color: var(--gray-900);
            line-height: 1.6;
        }

        /* ========================================
           PROFESSIONAL HEADER
           ======================================== */

        .storage-header {
            background: linear-gradient(135deg, var(--primary) 0%, var(--primary-light) 100%);
            box-shadow: var(--shadow-lg);
            position: sticky;
            top: 0;
            z-index: 1000;
            backdrop-filter: blur(10px);
        }

            .storage-header .navbar {
                padding: var(--space-md) 0;
            }

        /* Sidebar Toggle */
        .sidebar-toggle {
            width: 48px;
            height: 48px;
            display: flex;
            align-items: center;
            justify-content: center;
            background: rgba(255, 255, 255, 0.1);
            border: 1px solid rgba(255, 255, 255, 0.2);
            border-radius: var(--radius-lg);
            color: white;
            cursor: pointer;
            transition: all var(--transition-base);
            position: relative;
            overflow: hidden;
        }

            .sidebar-toggle::before {
                content: '';
                position: absolute;
                inset: 0;
                background: linear-gradient(135deg, var(--accent), var(--accent-light));
                opacity: 0;
                transition: opacity var(--transition-base);
            }

            .sidebar-toggle:hover::before {
                opacity: 1;
            }

            .sidebar-toggle i {
                font-size: 1.5rem;
                position: relative;
                z-index: 1;
            }

            .sidebar-toggle:hover {
                transform: translateY(-2px);
                box-shadow: 0 8px 16px rgba(6, 182, 212, 0.3);
            }

        /* Brand */
        .storage-brand {
            display: flex;
            align-items: center;
            gap: var(--space-md);
            text-decoration: none;
        }

        .brand-logo {
            width: 52px;
            height: 52px;
            display: flex;
            align-items: center;
            justify-content: center;
            background: linear-gradient(135deg, var(--accent), var(--accent-light));
            border-radius: var(--radius-xl);
            color: white;
            font-size: 1.75rem;
            box-shadow: 0 8px 24px rgba(6, 182, 212, 0.4);
            position: relative;
            overflow: hidden;
        }

            .brand-logo::before {
                content: '';
                position: absolute;
                top: -50%;
                left: -50%;
                width: 200%;
                height: 200%;
                background: linear-gradient(45deg, transparent, rgba(255, 255, 255, 0.2), transparent);
                animation: shimmer 3s infinite;
            }

        @@keyframes shimmer {
            0%

        {
            transform: translateX(-100%) translateY(-100%) rotate(45deg);
        }

        100% {
            transform: translateX(100%) translateY(100%) rotate(45deg);
        }

        }

        .brand-info {
            display: flex;
            flex-direction: column;
        }

        .brand-name {
            font-size: 1.375rem;
            font-weight: 700;
            color: white;
            line-height: 1.2;
            letter-spacing: -0.02em;
        }

        .brand-tagline {
            font-size: 0.8125rem;
            color: rgba(255, 255, 255, 0.7);
            font-weight: 500;
        }

        /* Header Navigation */
        .header-nav {
            display: flex;
            align-items: center;
            gap: var(--space-sm);
        }

        .nav-item-icon {
            position: relative;
            width: 44px;
            height: 44px;
            display: flex;
            align-items: center;
            justify-content: center;
            background: rgba(255, 255, 255, 0.1);
            border: 1px solid rgba(255, 255, 255, 0.2);
            border-radius: var(--radius-lg);
            color: white;
            cursor: pointer;
            transition: all var(--transition-base);
        }

            .nav-item-icon:hover {
                background: rgba(255, 255, 255, 0.2);
                transform: translateY(-2px);
            }

            .nav-item-icon i {
                font-size: 1.125rem;
            }

        /* Notification Badge */
        .badge-notification {
            position: absolute;
            top: -6px;
            right: -6px;
            min-width: 20px;
            height: 20px;
            display: flex;
            align-items: center;
            justify-content: center;
            padding: 0 0.375rem;
            background: linear-gradient(135deg, var(--danger), var(--danger-light));
            border: 2px solid var(--primary);
            border-radius: 999px;
            font-size: 0.6875rem;
            font-weight: 700;
            color: white;
            animation: bounce 2s infinite;
        }

        @@keyframes bounce {
            0%, 100%

        {
            transform: scale(1);
        }

        50% {
            transform: scale(1.1);
        }

        }

        /* Profile Button */
        .profile-btn {
            display: flex;
            align-items: center;
            gap: var(--space-sm);
            padding: 0.375rem 0.75rem;
            background: rgba(255, 255, 255, 0.1);
            border: 1px solid rgba(255, 255, 255, 0.2);
            border-radius: var(--radius-xl);
            color: white;
            cursor: pointer;
            transition: all var(--transition-base);
        }

            .profile-btn:hover {
                background: rgba(255, 255, 255, 0.2);
            }

        .profile-avatar {
            width: 36px;
            height: 36px;
            border-radius: 50%;
            border: 2px solid var(--accent);
            object-fit: cover;
        }

        .profile-info {
            display: flex;
            flex-direction: column;
            align-items: flex-start;
        }

        .profile-name {
            font-size: 0.875rem;
            font-weight: 600;
            line-height: 1.2;
        }

        .profile-role {
            font-size: 0.75rem;
            color: rgba(255, 255, 255, 0.7);
        }

        /* ========================================
           PROFESSIONAL SIDEBAR
           ======================================== */

        .storage-sidebar {
            width: 280px;
            background: white;
            box-shadow: var(--shadow-xl);
            border-right: 1px solid var(--gray-200);
        }

        .sidebar-header {
            padding: var(--space-xl);
            background: linear-gradient(135deg, var(--primary) 0%, var(--primary-light) 100%);
            position: relative;
            overflow: hidden;
        }

            .sidebar-header::before {
                content: '';
                position: absolute;
                top: -50%;
                right: -50%;
                width: 200%;
                height: 200%;
                background: radial-gradient(circle, rgba(255, 255, 255, 0.1) 0%, transparent 70%);
            }

        .sidebar-user {
            display: flex;
            align-items: center;
            gap: var(--space-md);
            position: relative;
            z-index: 1;
        }

        .sidebar-user-avatar {
            width: 56px;
            height: 56px;
            border-radius: 50%;
            border: 3px solid var(--accent);
            object-fit: cover;
            box-shadow: 0 4px 12px rgba(6, 182, 212, 0.4);
        }

        .sidebar-user-info {
            flex: 1;
        }

        .sidebar-user-name {
            font-size: 1rem;
            font-weight: 700;
            color: white;
            margin-bottom: 0.25rem;
        }

        .sidebar-user-email {
            font-size: 0.8125rem;
            color: rgba(255, 255, 255, 0.8);
        }

        .sidebar-user-status {
            width: 12px;
            height: 12px;
            background: var(--success);
            border: 3px solid white;
            border-radius: 50%;
            position: absolute;
            bottom: 0;
            right: 0;
            animation: pulse-status 2s infinite;
        }

        @@keyframes pulse-status {
            0%, 100%

        {
            box-shadow: 0 0 0 0 rgba(16, 185, 129, 0.7);
        }

        50% {
            box-shadow: 0 0 0 8px rgba(16, 185, 129, 0);
        }

        }

        /* Sidebar Navigation */
        .sidebar-nav {
            padding: var(--space-lg);
        }

        .nav-section {
            margin-bottom: var(--space-xl);
        }

        .nav-section-title {
            font-size: 0.75rem;
            font-weight: 700;
            color: var(--gray-500);
            text-transform: uppercase;
            letter-spacing: 0.05em;
            margin-bottom: var(--space-md);
            padding: 0 var(--space-md);
        }

        .nav-link {
            display: flex;
            align-items: center;
            gap: var(--space-md);
            padding: var(--space-md) var(--space-md);
            border-radius: var(--radius-lg);
            color: var(--gray-700);
            text-decoration: none;
            font-weight: 500;
            transition: all var(--transition-base);
            position: relative;
            margin-bottom: var(--space-xs);
        }

            .nav-link::before {
                content: '';
                position: absolute;
                left: 0;
                top: 50%;
                transform: translateY(-50%);
                width: 4px;
                height: 0;
                background: linear-gradient(180deg, var(--accent), var(--accent-light));
                border-radius: 0 4px 4px 0;
                transition: height var(--transition-base);
            }

            .nav-link:hover {
                background: var(--gray-100);
                color: var(--primary);
                transform: translateX(4px);
            }

                .nav-link:hover::before {
                    height: 60%;
                }

            .nav-link.active {
                background: linear-gradient(135deg, var(--accent) 0%, var(--accent-light) 100%);
                color: white;
                box-shadow: 0 4px 12px rgba(6, 182, 212, 0.3);
            }

                .nav-link.active::before {
                    height: 100%;
                    background: white;
                }

                .nav-link.active .nav-icon {
                    background: rgba(255, 255, 255, 0.2);
                    color: white;
                }

        .nav-icon {
            width: 40px;
            height: 40px;
            display: flex;
            align-items: center;
            justify-content: center;
            background: var(--gray-100);
            border-radius: var(--radius-md);
            transition: all var(--transition-base);
        }

            .nav-icon i {
                font-size: 1.125rem;
            }

        .nav-text {
            flex: 1;
        }

        .nav-badge {
            padding: 0.25rem 0.625rem;
            background: var(--danger);
            color: white;
            font-size: 0.6875rem;
            font-weight: 700;
            border-radius: 999px;
        }

        .nav-arrow {
            font-size: 0.875rem;
            transition: transform var(--transition-base);
        }

        .nav-link[aria-expanded="true"] .nav-arrow {
            transform: rotate(180deg);
        }

        /* Submenu */
        .nav-submenu {
            list-style: none;
            padding: var(--space-sm) 0 var(--space-sm) 3.5rem;
            margin: 0;
        }

        .nav-submenu-link {
            display: flex;
            align-items: center;
            gap: var(--space-sm);
            padding: var(--space-sm) var(--space-md);
            border-radius: var(--radius-md);
            color: var(--gray-600);
            text-decoration: none;
            font-size: 0.875rem;
            transition: all var(--transition-base);
        }

            .nav-submenu-link:hover {
                background: var(--gray-100);
                color: var(--accent);
                transform: translateX(4px);
            }

            .nav-submenu-link.active {
                background: rgba(6, 182, 212, 0.1);
                color: var(--accent);
                font-weight: 600;
            }

            .nav-submenu-link i {
                font-size: 0.5rem;
            }

        /* Storage Stats Card */
        .storage-stats-card {
            margin: var(--space-lg);
            padding: var(--space-xl);
            background: linear-gradient(135deg, var(--secondary) 0%, var(--secondary-light) 100%);
            border-radius: var(--radius-xl);
            color: white;
            box-shadow: var(--shadow-lg);
            position: relative;
            overflow: hidden;
        }

            .storage-stats-card::before {
                content: '';
                position: absolute;
                top: -50%;
                right: -20%;
                width: 150%;
                height: 150%;
                background: radial-gradient(circle, rgba(255, 255, 255, 0.1) 0%, transparent 70%);
            }

        .storage-icon {
            width: 56px;
            height: 56px;
            display: flex;
            align-items: center;
            justify-content: center;
            background: rgba(255, 255, 255, 0.2);
            border-radius: var(--radius-xl);
            margin-bottom: var(--space-md);
            position: relative;
            z-index: 1;
        }

            .storage-icon i {
                font-size: 1.75rem;
            }

        .storage-title {
            font-size: 0.875rem;
            font-weight: 600;
            margin-bottom: var(--space-sm);
            opacity: 0.9;
            position: relative;
            z-index: 1;
        }

        .storage-value {
            font-size: 1.75rem;
            font-weight: 700;
            margin-bottom: var(--space-xs);
            position: relative;
            z-index: 1;
        }

        .storage-label {
            font-size: 0.75rem;
            opacity: 0.8;
            position: relative;
            z-index: 1;
        }

        .storage-progress {
            height: 8px;
            background: rgba(255, 255, 255, 0.2);
            border-radius: 999px;
            overflow: hidden;
            margin-top: var(--space-md);
            position: relative;
            z-index: 1;
        }

        .storage-progress-bar {
            height: 100%;
            background: linear-gradient(90deg, white, rgba(255, 255, 255, 0.8));
            border-radius: 999px;
            transition: width var(--transition-slow);
            position: relative;
            overflow: hidden;
        }

            .storage-progress-bar::after {
                content: '';
                position: absolute;
                top: 0;
                left: 0;
                bottom: 0;
                right: 0;
                background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.3), transparent);
                animation: shimmer-progress 2s infinite;
            }

        @@keyframes shimmer-progress {
            0%

        {
            transform: translateX(-100%);
        }

        100% {
            transform: translateX(100%);
        }

        }

        /* ========================================
           PROFESSIONAL DROPDOWNS
           ======================================== */

        .professional-dropdown {
            min-width: 320px;
            background: white;
            border: none;
            box-shadow: var(--shadow-2xl);
            border-radius: var(--radius-xl);
            padding: var(--space-sm);
            margin-top: var(--space-md);
            animation: slideDown var(--transition-base);
        }

        @@keyframes slideDown {
            from

        {
            opacity: 0;
            transform: translateY(-10px);
        }

        to {
            opacity: 1;
            transform: translateY(0);
        }

        }

        .dropdown-header-pro {
            padding: var(--space-lg);
            background: linear-gradient(135deg, var(--primary) 0%, var(--primary-light) 100%);
            border-radius: var(--radius-lg);
            color: white;
            font-weight: 600;
            margin-bottom: var(--space-sm);
            display: flex;
            align-items: center;
            gap: var(--space-sm);
        }

            .dropdown-header-pro i {
                font-size: 1.125rem;
            }

        .dropdown-item-pro {
            display: flex;
            align-items: center;
            gap: var(--space-md);
            padding: var(--space-md);
            border-radius: var(--radius-lg);
            text-decoration: none;
            color: var(--gray-700);
            transition: all var(--transition-base);
            margin-bottom: var(--space-xs);
        }

            .dropdown-item-pro:hover {
                background: var(--gray-100);
                transform: translateX(4px);
            }

        .dropdown-item-icon {
            width: 40px;
            height: 40px;
            display: flex;
            align-items: center;
            justify-content: center;
            border-radius: var(--radius-md);
            flex-shrink: 0;
        }

            .dropdown-item-icon.primary {
                background: rgba(6, 182, 212, 0.1);
                color: var(--accent);
            }

            .dropdown-item-icon.success {
                background: rgba(16, 185, 129, 0.1);
                color: var(--success);
            }

            .dropdown-item-icon.warning {
                background: rgba(245, 158, 11, 0.1);
                color: var(--warning);
            }

            .dropdown-item-icon.danger {
                background: rgba(239, 68, 68, 0.1);
                color: var(--danger);
            }

        .dropdown-item-content {
            flex: 1;
        }

        .dropdown-item-title {
            font-weight: 600;
            color: var(--gray-900);
            margin-bottom: 0.125rem;
        }

        .dropdown-item-text {
            font-size: 0.875rem;
            color: var(--gray-600);
        }

        .dropdown-footer-pro {
            padding: var(--space-md);
            text-align: center;
            border-top: 1px solid var(--gray-200);
            margin-top: var(--space-sm);
        }

        .dropdown-footer-link {
            color: var(--accent);
            text-decoration: none;
            font-weight: 600;
            font-size: 0.875rem;
            transition: all var(--transition-base);
        }

            .dropdown-footer-link:hover {
                color: var(--accent-dark);
            }

        /* ========================================
           MAIN CONTENT AREA
           ======================================== */

        .main-content-pro {
            background: var(--gray-50);
            min-height: calc(100vh - 80px);
            padding: var(--space-2xl);
        }

        /* Page Header */
        .page-header-pro {
            margin-bottom: var(--space-2xl);
        }

        .page-title-pro {
            font-size: 2rem;
            font-weight: 700;
            color: var(--gray-900);
            margin-bottom: var(--space-sm);
        }

        .page-subtitle-pro {
            font-size: 1rem;
            color: var(--gray-600);
        }

        /* Stats Grid */
        .stats-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
            gap: var(--space-xl);
            margin-bottom: var(--space-2xl);
        }

        .stat-card {
            background: white;
            border-radius: var(--radius-xl);
            padding: var(--space-xl);
            box-shadow: var(--shadow-md);
            transition: all var(--transition-base);
            position: relative;
            overflow: hidden;
        }

            .stat-card::before {
                content: '';
                position: absolute;
                top: 0;
                left: 0;
                right: 0;
                height: 4px;
                background: linear-gradient(90deg, var(--accent), var(--accent-light));
                transform: scaleX(0);
                transform-origin: left;
                transition: transform var(--transition-base);
            }

            .stat-card:hover {
                transform: translateY(-4px);
                box-shadow: var(--shadow-xl);
            }

                .stat-card:hover::before {
                    transform: scaleX(1);
                }

        .stat-header {
            display: flex;
            justify-content: space-between;
            align-items: flex-start;
            margin-bottom: var(--space-lg);
        }

        .stat-icon-wrapper {
            width: 56px;
            height: 56px;
            display: flex;
            align-items: center;
            justify-content: center;
            border-radius: var(--radius-xl);
            font-size: 1.5rem;
        }

            .stat-icon-wrapper.primary {
                background: linear-gradient(135deg, var(--accent), var(--accent-light));
                color: white;
                box-shadow: 0 8px 16px rgba(6, 182, 212, 0.3);
            }

            .stat-icon-wrapper.success {
                background: linear-gradient(135deg, var(--success), var(--success-light));
                color: white;
                box-shadow: 0 8px 16px rgba(16, 185, 129, 0.3);
            }

            .stat-icon-wrapper.warning {
                background: linear-gradient(135deg, var(--warning), var(--warning-light));
                color: white;
                box-shadow: 0 8px 16px rgba(245, 158, 11, 0.3);
            }

            .stat-icon-wrapper.danger {
                background: linear-gradient(135deg, var(--danger), var(--danger-light));
                color: white;
                box-shadow: 0 8px 16px rgba(239, 68, 68, 0.3);
            }

        .stat-trend {
            padding: 0.375rem 0.75rem;
            border-radius: 999px;
